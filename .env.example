# Frontend Environment Variables
VITE_MANTICORE_PROXY_URL=/api
VITE_EMBEDDINGS_URL=/embeddings

# Python Server Environment Variables
# Server settings
PORT=3001
HOST=0.0.0.0

# Manticore Search settings
MANTICORE_HOST=127.0.0.1
MANTICORE_PORT=9308

# CORS settings (comma-separated list)
CORS_ORIGINS=http://localhost:7600,http://127.0.0.1:7600

# Model settings
MAX_MODELS_IN_MEMORY=3
DEFAULT_TEXT_MODEL=sentence-transformers/all-MiniLM-L6-v2
DEFAULT_IMAGE_MODEL=openai/clip-vit-base-patch32
MODEL_CACHE_DIR=./models

# Performance settings
REQUEST_TIMEOUT=300
MODEL_LOAD_TIMEOUT=600
EMBEDDING_CACHE_TTL=3600
MAX_BATCH_SIZE=32
MAX_TEXT_LENGTH=8192
REFINE_NO_TELEMETRY=true 